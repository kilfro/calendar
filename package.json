{
  "name": "calendar",
  "version": "1.0.0",
  "description": "Calendar demo app",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "test": "jest",
    "test:c": "jest --coverage",
    "test:w": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kilfro/calendar.git"
  },
  "keywords": [
    "react",
    "js",
    "webpack"
  ],
  "author": "Kirill Frolov <frolov.cyrill@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kilfro/calendar/issues"
  },
  "homepage": "https://github.com/kilfro/calendar#readme",
  "dependencies": {
    "bootstrap": "^4.5.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-bootstrap-icons": "^1.0.3-alpha5",
    "react-dom": "^16.13.1",
    "react-outside-click-handler": "^1.3.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/jest-dom": "^4.2.4",
    "babel-jest": "*",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "html-webpack-plugin": "^4.3.0",
    "jest": "*",
    "jest-enzyme": "^7.1.2",
    "jest-localstorage-mock": "^2.4.3",
    "jsdom": "^16.4.0",
    "less-loader": "^6.2.0",
    "prop-types": "^15.7.2",
    "redux-devtools": "^3.6.1",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "^@store(.*)$": "<rootDir>/src/store$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/dist/**",
      "!webpack.config.js",
      "!**/index.js"
    ]
  }
}
